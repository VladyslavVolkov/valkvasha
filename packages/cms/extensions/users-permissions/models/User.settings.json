{
  "kind": "collectionType",
  "collectionName": "users-permissions_user",
  "info": {
    "name": "user",
    "description": ""
  },
  "options": {
    "draftAndPublish": false,
    "timestamps": false
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmed": {
      "type": "boolean",
      "default": true,
      "configurable": false,
      "private": true
    },
    "password": {
      "type": "password",
      "required": false,
      "private": true,
      "default": "BNd1g1t@l"
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "required": true
    },
    "provider": {
      "type": "string",
      "private": true
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "private": true
    },
    "role": {
      "plugin": "users-permissions",
      "model": "role",
      "via": "users"
    },
    "firstName": {
      "type": "string",
      "required": false
    },
    "lastName": {
      "type": "string",
      "required": false
    },
    "photo": {
      "model": "file",
      "via": "related",
      "allowedTypes": ["images"],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    }
  }
}
